[epel-testing]
name=Extra Packages for Enterprise Linux 4 - Testing - $basearch 
#baseurl=http://download.fedoraproject.org/pub/epel/testing/4/$basearch
mirrorlist=http://mirrors.fedoraproject.org/mirrorlist?repo=testing-epel4&arch=$basearch
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL

